{

  "version": 2,

 
  "builds": [
    {
    
      "src": "/server/index.js",

     
      "use": "@vercel/node"
    },
    {
   
      "src": "dist/public/**",

      
      "use": "@vercel/static"
    }
  ],

  
  "routes": [
    {
      
      "src": "/server/routes.js",

      
      "dest": "dist/server/index.js"
    },
    {
      
      "src": "/(.*)",

     
      "dest": "dist/public/index.html"
    }
  ]
}